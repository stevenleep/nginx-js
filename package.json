{
  "name": "nginx-js",
  "version": "1.0.0",
  "type": "module",
  "description": "Nginx for JavaScript - A powerful request proxy library for browser with plugin architecture",
  "keywords": [
    "nginx",
    "nginx-js",
    "gateway",
    "proxy",
    "interceptor",
    "request",
    "fetch",
    "xhr",
    "websocket",
    "plugin",
    "middleware",
    "typescript",
    "browser"
  ],
  "author": {
    "name": "Steven Lee",
    "email": "lsw@zstun.com",
    "url": "https://github.com/stevenleep"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stevenleep/nginx-js.git",
    "directory": "."
  },
  "bugs": {
    "url": "https://github.com/stevenleep/nginx-js/issues"
  },
  "homepage": "https://github.com/stevenleep/nginx-js#readme",
  "private": true,
  "files": [
    "README.md",
    "LICENSE",
    "packages"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "build:watch": "pnpm -r build:watch",
    "build:prod": "pnpm -r build:prod",
    "dev": "pnpm -r build:watch",
    "type-check": "pnpm -r type-check",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "check": "pnpm run type-check && pnpm run lint && pnpm run format:check",
    "clean": "pnpm -r clean",
    "prepublishOnly": "echo 'This is a monorepo. Publish individual packages instead.' && exit 1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "rollup": "^4.9.0",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/stevenleep"
  }
}
